<?xml version="1.0" encoding="UTF-8"?>
<scene version="1.0">
  <settings>
    <output simulation_name="prismatic_press" enable_output="false" output_directory="./output/rb_constraints/prismatic_press" codegen_directory="./codegen/rb_constraints" />
    <simulation init_frictional_contact="false" />
    <execution end_simulation_time="5.0" />
  </settings>
  <objects>
    <rigidbodies>
      <rigidbody id="box0" label="box0" mass="1.0">
        <geometry type="box">
          <size x="0.1" y="0.1" z="0.1" />
        </geometry>
      </rigidbody>
      <rigidbody id="box1" label="box1" mass="1.0">
        <geometry type="box">
          <size x="0.1" y="0.1" z="0.1" />
        </geometry>
        <transform translation="0,0,-0.5" />
      </rigidbody>
    </rigidbodies>
  </objects>
  <boundary_conditions>
    <rigidbody_constraint type="fix" target="box0" />
    <rigidbody_constraint type="prismatic_press" body_a="box0" body_b="box1" pivot="0,0,0" axis="0,0,1" target_velocity="0.2" max_force="20.0" />
    <rigidbody_constraint type="spring" body_a="box0" body_b="box1" pivot="0,0,0" axis="0,0,-0.5" stiffness="8.0" damping="0.5" />
  </boundary_conditions>
</scene>
